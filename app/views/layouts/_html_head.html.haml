%title
  = yield(:title) || APP_CONFIG[:app_name]
%meta{"http-equiv"=>"Content-Type", :content=>"text/html; charset=utf-8"}
%meta(property="og:title" content="Parkerhill Tech (Jonathan Linowes)")
%meta(property="og:type" content="business")
%meta(property="og:url" content="http://parkerhill.com")
%meta(property="og:image" content="http://parkerhill.com/images/rooster.jpg")
%meta property="og:site_name" content="Parkerhill Tech (Jonathan Linowes)"
-#%meta property="fb:admins" content="USER_ID"
%meta property="og:description"
      content="Hello, my name is Jon Linowes. I build web applications. I have expertise with a broad range of software and Internet technologies including Ruby on Rails, jQuery, and Rspec."


= stylesheet_link_tag 'compiled/application', :media => 'screen, projection'
= stylesheet_link_tag 'compiled/print', :media => 'print'
= render "layouts/ie"
= render "layouts/html5"
=# stylesheet_link_tag 'smoothness/jquery-ui-1.8.9.custom', :media => 'screen, projection'
= yield :stylesheets
= javascript_include_tag :defaults
%script( src="http://connect.facebook.net/en_US/all.js#xfbml=1")
= yield :javascript_includes
%script
  = yield :javascripts
  $(document).ready(function(){
  $('button, input:submit, .button_link').button();     
  = yield :javascript_ready 
  = yield :javascript_ready_last
  });
= csrf_meta_tag
= yield :head

