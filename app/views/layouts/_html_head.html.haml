%title
  = yield(:title) || APP_CONFIG[:app_name]
%meta{"http-equiv"=>"Content-Type", :content=>"text/html; charset=utf-8"}/
= stylesheet_link_tag 'compiled/application', :media => 'screen, projection'
= stylesheet_link_tag 'compiled/print', :media => 'print'
= render "layouts/ie"
= render "layouts/html5"
=# stylesheet_link_tag 'smoothness/jquery-ui-1.8.9.custom', :media => 'screen, projection'
= yield :stylesheets
= javascript_include_tag :defaults
= javascript_include_tag 'jquery-ui-1.8.9.custom.min'
= yield :javascript_includes
%script
  = yield :javascripts
  $(document).ready(function(){
  $('button, input:submit, .button_link').button();     
  = yield :javascript_ready 
  = yield :javascript_ready_last
  });
= csrf_meta_tag
= yield :head

